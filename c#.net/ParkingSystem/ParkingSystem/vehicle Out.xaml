        <Label Content="  Token NO:" HorizontalAlignment="Center" Width="100" Height="30" Margin="225,117,475,303" FontWeight="Bold" ></Label>
        <Label Content="  (Or)" HorizontalAlignment="Center" Width="100" Height="30" Margin="239,152,461,268" FontWeight="Bold" ></Label>
        <Label Content="   Vehicle No:" HorizontalAlignment="Center" Width="100" Height="30" Margin="225,187,475,233" FontWeight="Bold" ></Label>
        <Label Content="  Time Out:" HorizontalAlignment="Center" Width="100" Height="30" Margin="225,227,475,193" FontWeight="Bold"></Label>
        <Label Content="  Duration:" HorizontalAlignment="Center" Width="100" Height="30" Margin="225,262,475,158" FontWeight="Bold"></Label>
