<Window x:Class="ParkingSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParkingSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
       <StackPanel>
           
            <StackPanel x:Name="LoginPage" Visibility="Visible">
                <local:LoginPage/>
            </StackPanel>

            <StackPanel x:Name="vehicleSlot" Visibility="Visible">
               <local:vehicleSlot/>
            </StackPanel>
            
            <StackPanel x:Name="Vehicle_In" Visibility="Collapsed">
              <local:Vehicle_In/>
            </StackPanel>
            
            <StackPanel x:Name="Vehicle_Out" Visibility="Collapsed">
               <local:vehicle_Out/>
            </StackPanel>
            
            <StackPanel x:Name="ViewSlot" Visibility="Collapsed">
               <local:ViewSlot/>
            </StackPanel>
            
            <StackPanel x:Name="Receipt" Visibility="Collapsed">
               <local:Receipt/>
            </StackPanel>
           
        </StackPanel>
    </Grid>
</Window>
